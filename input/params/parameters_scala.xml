<?xml version="1.0" encoding="UTF-8"?>
<ParameterHandler>
    <Algebraic_20Solver>
        <Max_20iterations>
            <value>10000</value>
            <default_value>500</default_value>
            <documentation>The maximum number of iterations to be performed by the linear solver</documentation>
            <pattern>0</pattern>
            <pattern_description>[Integer range -2147483648...2147483647 (inclusive)]</pattern_description>
        </Max_20iterations>
        <Preconditioner>
            <value>Jacobi</value>
            <default_value>Jacobi</default_value>
            <documentation>The preconditioner to be used with the linear solver</documentation>
            <pattern>3</pattern>
            <pattern_description>[Selection Jacobi|Chebyshev ]</pattern_description>
        </Preconditioner>
        <Solver>
            <value>GMRES</value>
            <default_value>GMRES</default_value>
            <documentation>The linear solver to be used</documentation>
            <pattern>2</pattern>
            <pattern_description>[Selection GMRES|CG ]</pattern_description>
        </Solver>
        <Tolerance>
            <value>1e-06</value>
            <default_value>1.0e-6</default_value>
            <documentation>The tolerance for the linear solver (it will be multiplied by the L^2 norm of the rhs)</documentation>
            <pattern>1</pattern>
            <pattern_description>[Double -MAX_DOUBLE...MAX_DOUBLE (inclusive)]</pattern_description>
        </Tolerance>
    </Algebraic_20Solver>
    <Data>
        <Advection_20Coefficient_20x_20value>
            <value>1.0</value>
            <default_value>0</default_value>
            <documentation>The x value of the advection coefficient of the PDE</documentation>
            <pattern>6</pattern>
            <pattern_description>[Anything]</pattern_description>
        </Advection_20Coefficient_20x_20value>
        <Advection_20Coefficient_20y_20value>
            <value>0</value>
            <default_value>0</default_value>
            <documentation>The y value of the advection coefficient of the PDE</documentation>
            <pattern>7</pattern>
            <pattern_description>[Anything]</pattern_description>
        </Advection_20Coefficient_20y_20value>
        <Advection_20Coefficient_20z_20value>
            <value>0</value>
            <default_value>0</default_value>
            <documentation>The z value of the advection coefficient of the PDE</documentation>
            <pattern>8</pattern>
            <pattern_description>[Anything]</pattern_description>
        </Advection_20Coefficient_20z_20value>
        <Diffusion_20Coefficient>
            <value>2.0</value>
            <default_value>1</default_value>
            <documentation>The diffusion coefficient of the PDE</documentation>
            <pattern>4</pattern>
            <pattern_description>[Anything]</pattern_description>
        </Diffusion_20Coefficient>
        <Dirichlet_20Boundary_20function>
            <value>0</value>
            <default_value>0</default_value>
            <documentation>The function to be applied at the Dirichlet boundary</documentation>
            <pattern>10</pattern>
            <pattern_description>[Anything]</pattern_description>
        </Dirichlet_20Boundary_20function>
        <Force_20term>
            <value>4.0</value>
            <default_value>0</default_value>
            <documentation>The force term (aka the rhs of the equation: Lu=f in Omega )</documentation>
            <pattern>9</pattern>
            <pattern_description>[Anything]</pattern_description>
        </Force_20term>
        <Neumann_20Boundary_20function>
            <value>0</value>
            <default_value>0</default_value>
            <documentation>The function to be applied at the Neumann boundary</documentation>
            <pattern>11</pattern>
            <pattern_description>[Anything]</pattern_description>
        </Neumann_20Boundary_20function>
        <Reaction_20Coefficient>
            <value>1.0</value>
            <default_value>0</default_value>
            <documentation>The reaction coefficient of the PDE</documentation>
            <pattern>5</pattern>
            <pattern_description>[Anything]</pattern_description>
        </Reaction_20Coefficient>
    </Data>
</ParameterHandler>
